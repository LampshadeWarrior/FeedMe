<p id="notice"><%= notice %></p>

<div class="feed-table">
  <h1>Feeds</h1>
  <div class="feed-box">
    <table>
      <thead>
        <tr>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @feeds.each do |feed| %>
          <tr>
            <td><%= link_to feed.name, entries_path(feed), class: "link" %></td>
            <td><%= feed.description %></td>
            <td><%= link_to 'Edit', edit_feed_path(feed), class: "link" %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
  <br>

  <%= link_to 'New Feed', new_feed_path, class: "link" %>
</div>
